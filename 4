class AdjMatrix :
	def __init__( self, numOfVertex, source ) :
		self.source = source
		self.matrix = [[-1 for i in range( numOfVertex )] for j in range(numOfVertex)]
		
	def AddEdge( self, source, goal, weight ) :
		self.matrix[source][goal] = weight 
  
if __name__ == '__main__' :
	numOfVertex = int( input() ) 
	numOfEdges = int( input() ) 
	sourceVertex = int( input() )  
	adjMatrix = AdjMatrix( numOfVertex, sourceVertex )

	while True :
		input_list = input().split()
		if input_list[0] == '0' :
			break
		
		adjMatrix.AddEdge( int( input_list[0] )-1, int( input_list[1] )-1, int( input_list[2] )-1 )
		print( adjMatrix.matrix )
		
		
